    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 ./src/",
    "lint-fix": "yarn eslint ./src/ --fix"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@koa/cors": "^5.0.0",
    "@pelevesque/elo": "^2.0.9",
    "@standard-crypto/farcaster-js": "^7.2.0",
    "amala": "^10.0.8",
    "axios": "^1.6.7",
    "canvas": "^2.11.2",
    "dotenv": "^16.4.1",
    "download": "^8.0.0",
    "envalid": "^8.0.0",
    "koa": "^2.15.0",
    "koa-bodyparser": "^4.4.1",
    "koa-router": "^12.0.1",
    "mongoose": "^8.1.1",
    "ultimate-text-to-image": "^1.0.1"
  },
  "devDependencies": {
    "@big-whale-labs/bwl-eslint-backend": "^1.0.5",
    "@big-whale-labs/prettier-config": "^1.1.3",
    "@rushstack/eslint-patch": "^1.7.2",
    "@typegoose/typegoose": "^12.1.0",
    "@types/download": "^8",
    "@types/koa": "^2.14.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.8",
    "@types/koa__cors": "^5.0.0",
    "@types/node": "^20.11.16",
    "eslint": "^8.56.0",
    "module-alias": "^2.2.3",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "packageManager": "yarn@4.1.0"
}
