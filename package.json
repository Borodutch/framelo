  "author": "backmeupplz <backmeupplz@gmail.com>",
  "prettier": "@big-whale-labs/prettier-config",
  "license": "MIT",
  "scripts": {
    "distribute": "yarn build && node dist/server.js",
    "start": "tsc-watch --skipLibCheck --onSuccess 'node dist/server.js'",
